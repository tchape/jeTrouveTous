pipeline{
    agent any

    stages{
         stage('compilation du projet'){
             steps{
                  sh 'mvn clean compile'
            /*    withMaven(maven : 'maven_3_6_1'){
                    echo "step clean compile"
                }*/
             }

         }

         stage('test du projet '){
            steps{
              /*  withMaven(maven : 'maven_3_6_1'){
                    echo "step test"
                }*/

                    sh 'mvn test'

            }
         }

          stage('Deploiement du projet '){
                     steps{
                         echo "debut du deploiement avec maven"

                            sh 'mvn deploy'
                         /*withMaven(maven : 'maven_3_6_1'){
                            echo "step deploy"
                         }*/

                     }
                  }
    }
}